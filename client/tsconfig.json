{
    "compilerOptions": {
        "target": "ES5",
        "lib": [
            "dom",
            "DOM.Iterable",
            "ESNext"
        ],
        "allowJs": false,
        "esModuleInterop": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true,
        "module": "ESNext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "jsx": "react-jsx"
    },
    "include": [
        "src"
    ]
}